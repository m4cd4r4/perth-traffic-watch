{
  "version": 2,
  "name": "swanflow",
  "builds": [
    {
      "src": "frontend/web-dashboard/**",
      "use": "@vercel/static"
    },
    {
      "src": "frontend/landing/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/landing",
      "dest": "/frontend/landing/index.html"
    },
    {
      "src": "/landing/(.*)",
      "dest": "/frontend/landing/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/web-dashboard/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" },
        { "key": "Pragma", "value": "no-cache" },
        { "key": "Expires", "value": "0" }
      ]
    },
    {
      "source": "/",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" },
        { "key": "Pragma", "value": "no-cache" },
        { "key": "Expires", "value": "0" }
      ]
    },
    {
      "source": "/landing",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" },
        { "key": "Pragma", "value": "no-cache" },
        { "key": "Expires", "value": "0" }
      ]
    },
    {
      "source": "/(.*)\\.css",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    },
    {
      "source": "/(.*)\\.js",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ]
}
