{
  "version": 2,
  "name": "swanflow",
  "builds": [
    {
      "src": "frontend/web-dashboard/**",
      "use": "@vercel/static"
    },
    {
      "src": "frontend/landing/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/landing",
      "destination": "/frontend/landing/index.html"
    },
    {
      "source": "/landing/:path*",
      "destination": "/frontend/landing/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/frontend/web-dashboard/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" },
        { "key": "Pragma", "value": "no-cache" },
        { "key": "Expires", "value": "0" }
      ]
    },
    {
      "source": "/",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" },
        { "key": "Pragma", "value": "no-cache" },
        { "key": "Expires", "value": "0" }
      ]
    },
    {
      "source": "/landing",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" },
        { "key": "Pragma", "value": "no-cache" },
        { "key": "Expires", "value": "0" }
      ]
    },
    {
      "source": "/(.*)\\.css",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    },
    {
      "source": "/(.*)\\.js",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ]
}
